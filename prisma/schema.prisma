datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  password  String
  createdAt DateTime @default(now())
}

model Person {
  id        Int      @id @default(autoincrement())
  name      String
  image     String?
  createdAt DateTime @default(now())
}
